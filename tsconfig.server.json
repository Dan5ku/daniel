{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "server-dist",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["server"]
}